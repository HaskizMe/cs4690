{
    "name": "node-starter",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "server": "nodemon --exec ts-node server/server.ts",
        "build": "tsc",
        "client": "npm run dev --prefix client",
        "start": "node dist/server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "mongodb": "^6.20.0",
        "mongoose": "^8.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.6",
        "concurrently": "^9.2.1",
        "json-server": "0.17.0",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
